<h1>Listing timelines</h1>

<table class="table-striped pad">
  <thead>
    <tr>
      <th>Event date</th>
      <th>Event</th>
      <th>Who</th>
      <th>Notes</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @timelines.each do |timeline| %>
      <tr>
        <td class="pad" width="100"><%= timeline.event_year %></td>
        <td width="170"><%= timeline.event %></td>
        <td width="170"><%= timeline.who %></td>
        <td width="200"><%= timeline.notes %></td>
        <td class="pad"><%= link_to 'Show', timeline %></td>
        <td><%= link_to 'Edit', edit_timeline_path(timeline) %></td>
        <td><%= link_to 'Destroy', timeline, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timeline', new_timeline_path %>
